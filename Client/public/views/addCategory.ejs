    <!-- Contact Section -->
    <section id="contact">
        <div class="container">
            <div class="row" style="margin-top: 3%;">
                <div class="col-lg-12 text-center">
                    <h2>Create a Service</h2>
                    <hr class="star-primary">
                </div>
            </div>
            <div class="row">
                <div class="col-lg-8 col-lg-offset-2">
                    <!-- To configure the contact form email address, go to mail/contact_me.php and update the email address in the PHP file on line 19. -->
                    <!-- The form should work on most web servers, but if the form is not working you may need to configure your web server differently. -->
                    <form name="sentMessage" id="contactForm">

                        <div class="row control-group">
                            <div class="form-group col-xs-12 floating-label-form-group controls">
                                <label for="name">Name</label>
                                <input type="name" ng-model="formData.servicename" class="form-control"  placeholder="Enter service name" id="name"><!-- required data-validation-required-message="Please enter your email address.">-->
                                <p class="help-block text-danger"></p>
                            </div>
                        </div>


                     <div class="row control-group">
                            <div class="form-group col-xs-12 floating-label-form-group controls">
                                <label for="address">Address</label>
                                <input type="address" ng-model="formData.address" class="form-control"  placeholder="Address" id="address"><!-- required data-validation-required-message="Please enter your email address.">-->
                                <p class="help-block text-danger"></p>
                            </div>
                        </div>
                    <div class="row control-group">
                            <div class="form-group col-xs-12 floating-label-form-group controls">
                                <label for="phone">Phone No</label>
                                <input type="phone" class="form-control" ng-model="formData.contact"  placeholder="Phone No" id="phone"><!-- required data-validation-required-message="Please enter your email address.">-->
                                <p class="help-block text-danger"></p>
                            </div>
                        </div>

                  <!-- <div class="row control-group">
                            <div class="form-group col-xs-12 floating-label-form-group controls">
                                <label for="image">Upload Image</label>
                                <input type="file" class="form-control" style="font-family:Times New Roman;color:#919599"   placeholder="Upload Image" id="image"><!-- required data-validation-required-message="Please enter your email address.">-->
                                <!-- </input>
                                <p class="help-block text-danger"></p>
                            </div>
                        </div> -->

                        <div class="row control-group">
                          <div class="form-group col-xs-12 floating-label-form-group controls">

                              <select ng-model="categoryChoiceSelected">
                                      <option selected="selected">What would you like to create ?</option>
                                       <option value="new">Create a new category</option>
                                       <option value="existing">Choose from an Existing Category</option>

                                  </select>
                              <p class="help-block text-danger"></p>
                          </div>
                      </div>

                        <div class="row control-group" ng-if="categoryChoiceSelected == 'new' && categoryChoiceSelected != 'existing' ">
                          <div class="form-group col-xs-12 floating-label-form-group controls">
                              <label for="newCatg">New Category</label>
                              <input type="newCateg" ng-model="formData.categoryEntered" class="form-control"  placeholder="New Category Name" id="newCategory">
                              <p class="help-block text-danger" ng-if="categorynamealreadyexist">Category name already exist, please choose a different name</p>
                          </div>
                      </div>


                        <div class="row control-group"  ng-if="categoryChoiceSelected == 'existing' && categoryChoiceSelected != 'new' ">
                          <div class="form-group col-xs-12 floating-label-form-group controls">

                              <select ng-model="formData.categoryselected">
                                       <option selected="selected">Choose a category</option>
                                       <option value="{{category._id}}" ng-repeat="categoryToChoose in categories">{{categoryToChoose.name}}</option>

                            </select>
                              <p class="help-block text-danger"></p>
                          </div>
                      </div>



                        <div class="row control-group">
                            <div class="form-group col-xs-12 floating-label-form-group controls">
                                <label for="desc">Description</label>
                                <textarea rows="1" ng-model="formData.description" class="form-control" cols="50" name="description" placeholder ="Enter the description" style="font-family:Times New Roman;color:#919599" ></textarea>
                               <!-- <input type="tel" class="form-control" placeholder="Password" id="password" >--><!--required data-validation-required-message="Please enter your password">-->
                                <p class="help-block text-danger"></p>
                            </div>
                        </div>



                        <br>
                        <div id="success"></div>
                        <div class="row">
                            <div ><!--class="form-group col-xs-12">-->
                                <button type="submit" class="btn btn-success btn-lg" ng-click="addServiceAndCategory()">Add Cluster</button>

                            </div>


                        </div>
                    </form>
                </div>
            </div>
        </div>
    </section>

